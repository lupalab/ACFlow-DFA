{
    "gpu": "0",
    "seed": 123,
    "dataset": "vec",
    "dfile": "./data/cls/syn.pkl",
    "dimension": 10,
    "n_classes": 2,
    "class_weights": [1,1],
    "batch_size": 1000,
    "model": "acflow_classifier",
    "lambda_mse": 0.0,
    "lambda_xent": 1.0,
    "lambda_nll": 1.0,
    "transform": ["AF","TL","TL","TL","TL","AF"],
    "layer_cfg": ["ML","LR","CP2"],
    "rnncp_units": 256,
    "rnncp_layers": 2,
    "linear_hids": [256,256],
    "linear_rank": -1,
    "affine_hids": [256,256],
    "coupling_hids": [256,256],
    "prior": "autoreg",
    "n_components": 40,
    "prior_units": 256,
    "prior_layers": 2,
    "prior_hids": [256,256],
    "epochs": 500,
    "optimizer": "adam",
    "lr": 0.001,
    "decay_steps": 10000,
    "decay_rate": 0.5,
    "clip_gradient": 1,
    "exp_dir": "./exp/syn",
    "summ_freq": 100,
    "num_samples": 10
}